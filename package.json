{
  "name": "age",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "clean": "npx rimraf .next build out tmp-npm-cache && eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-pattern 'out/**' --ignore-pattern '.next/**' --ignore-pattern 'build/**' --ignore-pattern 'node_modules/**' --ignore-pattern 'next-env.d.ts' --ignore-pattern 'tmp-npm-cache' --ignore-pattern 'scripts/**' && node ./scripts/strip-comments-and-console.js && prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md,mdx}\"",
    "build": "npm run clean && npm run format:check && next build && next export -o out",
    "build:full": "npm run clean && npm run format:check && next build",
    "export": "npm run clean && npm run format:check && next build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern 'out/**' --ignore-pattern '.next/**' --ignore-pattern 'build/**' --ignore-pattern 'node_modules/**' --ignore-pattern 'next-env.d.ts' --ignore-pattern 'scripts/**'",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-pattern 'out/**' --ignore-pattern '.next/**' --ignore-pattern 'build/**' --ignore-pattern 'node_modules/**' --ignore-pattern 'next-env.d.ts' --ignore-pattern 'scripts/**'",
    "lint:scripts": "eslint scripts --env node --ext .js,.cjs --rule '{\"@typescript-eslint/no-require-imports\": 0}' || true",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md,mdx}\""
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "firebase": "^11.9.1",
    "fs-extra": "^11.3.2",
    "globby": "^14.1.0",
    "next": "^14.2.30",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
